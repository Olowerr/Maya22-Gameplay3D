form physicsScene
{
    layout = LAYOUT_VERTICAL
    width = 200
    alignment = ALIGN_BOTTOM_LEFT

    button shapeButton
    {
        width = 100%
        text = Sphere
    }
    
    button throwButton : shapeButton
    {
        text = Throw
    }
    
    button wireframeButton  : shapeButton
    {
        text = Solid
    }

    button drawDebugButton : shapeButton
    {
        text = Normal
    }

}